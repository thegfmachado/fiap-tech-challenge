{
  "name": "fiap-tech-challenge",
  "version": "0.0.1",
  "private": true,
  "description": "FIAP Front-end engeneering tech challenge",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thegfmachado/fiap-tech-challenge.git",
    "directory": "."
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "devDependencies": {
    "@fiap-tech-challenge/eslint-config": "file:./packages/eslint-config",
    "@fiap-tech-challenge/typescript-config": "file:./packages/typescript-config",
    "cspell": "^8.19.4",
    "prettier": "^3.5.3",
    "turbo": "2.5.3",
    "typescript": "^5"
  },
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=20"
  }
}